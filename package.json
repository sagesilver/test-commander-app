{
  "name": "test-commander",
  "version": "1.0.0",
  "description": "Modern test case and defect management platform",
  "private": true,
  "dependencies": {
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5",
    "file-saver": "^2.0.5",
    "@tanstack/react-table": "^8.14.1",
    "@tanstack/react-virtual": "^3.0.0",
    "firebase": "^9.23.0",
    "framer-motion": "^6.3.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.0",
    "serve": "^14.2.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.7.0"
  },
  "scripts": {
    "start": "set FAST_REFRESH=true && set GENERATE_SOURCEMAP=false && set CHOKIDAR_USEPOLLING=false && react-scripts start",
    "start:fast": "set FAST_REFRESH=true && set GENERATE_SOURCEMAP=false && set CHOKIDAR_USEPOLLING=false && set SKIP_PREFLIGHT_CHECK=true && react-scripts start",
    "build": "react-scripts build",
    "build:fast": "set GENERATE_SOURCEMAP=false && node --no-deprecation ./node_modules/.bin/react-scripts build",
    "serve": "serve -s build -l 3000",
    "build:serve": "npm run build:fast && npm run serve",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
